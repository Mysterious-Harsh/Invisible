<?xml version='1.0' encoding='utf-8'?>
<interface version="1.0">
  <object class="ttk.Frame" id="frame_1">
    <property name="height">200</property>
    <property name="relief">flat</property>
    <property name="width">200</property>
    <layout manager="place">
      <property name="anchor">nw</property>
      <property name="height">500</property>
      <property name="width">500</property>
      <property name="x">0</property>
      <property name="y">0</property>
    </layout>
    <child>
      <object class="tk.Listbox" id="dirlist_LB">
        <property name="font">{Arial} 12 {bold}</property>
        <property name="height">12</property>
        <property name="relief">flat</property>
        <property name="selectmode">single</property>
        <property name="state">normal</property>
        <property name="takefocus">true</property>
        <property name="width">25</property>
        <property name="xscrollcommand">scrollbar_H</property>
        <property name="yscrollcommand">scrollbar_Y</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="open_B">
        <property name="command">open</property>
        <property name="text" translatable="yes">Open</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.26</property>
          <property name="rely">0.7</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="close_B">
        <property name="command">close</property>
        <property name="text" translatable="yes">Close</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.07</property>
          <property name="rely">0.7</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Scrollbar" id="scrollbar_V">
        <property name="orient">vertical</property>
        <layout manager="pack">
          <property name="expand">true</property>
          <property name="fill">y</property>
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
